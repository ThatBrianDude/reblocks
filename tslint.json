{
  "extends": ["tslint:latest", "tslint-config-prettier", "tslint-react"],
  "rules": {
    "interface-name": [false],
    "jsx-no-multiline-js": false,
    "max-line-length": [true, 120],
    "member-access": [false],
    "no-any": true,
    "no-console": false,
    "no-implicit-dependencies": [true, "dev"],
    "no-submodule-imports": false
  }
}
